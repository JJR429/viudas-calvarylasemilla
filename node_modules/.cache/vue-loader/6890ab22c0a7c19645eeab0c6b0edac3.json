-if=\"this.$route.params.cat == 2\">\n        Estos son grados de nivel Secundaria\n      </p>\n      <div class=\"grados\">\n        <div\n          class=\"grado\"\n          v-for=\"materia in materias\"\n          v-bind:todo=\"materia\"\n          v-bind:key=\"materia.id\"\n        >\n          <router-link\n            :to=\"{ name: 'gradosDetail', params: { gradoSlug: materia.slug } }\"\n          >\n            <h2>{{ materia.name }}</h2>\n          </router-link>\n        </div>\n      </div>\n    </div>\n    <div class=\"container\" v-if=\"error\">\n      <h3>{{ error }}</h3>\n    </div>\n    <router-view :key=\"$route.path\" />\n  </div>\n</template>\n<script>\nimport GoBack from \"../components/GoBack\";\nexport default {\n  components: {\n    GoBack\n  },\n  data() {\n    return {\n      materias: null,\n      loading: false,\n      error: null\n    };\n  },\n  props: {\n    gradoSlug: {\n      type: String,\n      required: false\n    },\n    cat: {\n      type: String,\n      required: false\n    }\n  },\n  created() {\n    this.getData();\n  },\n  methods: {\n    getData: async function() {\n      this.loading = true;\n      const response = await fetch(\n        \"http://localhost/studia/api/actions/materias.php?grado=\" +\n          this.gradoSlug +\n          \"&cat=\" +\n          this.cat\n      );\n      const data = await response.json();\n      this.materias = await data;\n      if (this.grados.length > 0) {\n        this.loading = false;\n      } else {\n        this.loading = false;\n        this.error = \"No Existe este Grado\";\n        this.materias = null;\n      }\n      // // condici√≥n ? expr1 : expr2\n      // this.catId == 1 ? this.slug = 'primaria' : 'secundiaria'\n    }\n  }\n};\n</script>\n<style scoped>\n.grados {\n  display: flex;\n  justify-content: space-around;\n}\n.categorias {\n  display: flex;\n  justify-content: space-around;\n}</style\n>>\n"]}]}