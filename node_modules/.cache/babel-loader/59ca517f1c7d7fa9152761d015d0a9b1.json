ias = await data;\n      if (this.grados.length > 0) {\n        this.loading = false;\n      } else {\n        this.loading = false;\n        this.error = \"No Existe este Grado\";\n        this.materias = null;\n      }\n      // // condici√≥n ? expr1 : expr2\n      // this.catId == 1 ? this.slug = 'primaria' : 'secundiaria'\n    }\n  }\n};\n</script>\n<style scoped>\n.grados {\n  display: flex;\n  justify-content: space-around;\n}\n.categorias {\n  display: flex;\n  justify-content: space-around;\n}</style\n>>\n"],"sourceRoot":"src/views"}]}