AAA,CAAA,IAAA,CAAA,KAAA,EAAA;AACA,0BAAA,KAAA,CAAA,WAAA,IAAA,SAAA,EAAA;AACA,wBAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,QAAA,IAAA,QAAA;AACA;;AACA,0BAAA,KAAA,CAAA,WAAA,IAAA,aAAA,EAAA;AACA,wBAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CACA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,QAAA,IACA,kCAFA;AAIA;;AACA,0BAAA,KAAA,CAAA,WAAA,IAAA,YAAA,EAAA;AACA,wBAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CACA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,QAAA,IACA,iCAFA;AAIA;AACA,qBAhBA,MAgBA;AACA;AACA;AACA,sBAAA,KAAA,CAAA,KAAA,GAAA,IAAA;;AACA,0BAAA,QAAA,CAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,wBAAA,KAAA,CAAA,aAAA,GACA,mEADA;AAEA,uBAHA,MAGA;AACA,wBAAA,KAAA,CAAA,aAAA,GACA,2CADA;AAEA;AACA;AACA,mBAtCA,EAuCA,KAvCA,CAuCA,UAAA,QAAA,EAAA;AACA;AACA,oBAAA,KAAA,CAAA,KAAA,GAAA,IAAA;AACA,oBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,mBA3CA;AA4CA;;AA3DA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA4DA,KA5GA;AA6GA,IAAA,aAAA,EAAA,uBAAA,KAAA,EAAA;AACA,UAAA,EAAA,GAAA,sJAAA;AAEA,aAAA,EAAA,CAAA,IAAA,CAAA,KAAA,CAAA;AACA,KAjHA;AAkHA,IAAA,YAAA,EAAA,sBAAA,IAAA,EAAA;AACA,aAAA,IAAA,CAAA,MAAA,GAAA,CAAA,GAAA,IAAA,GAAA,KAAA;AACA,KApHA;AAqHA,IAAA,YAAA,EAAA,sBAAA,IAAA,EAAA;AACA,aAAA,IAAA,CAAA,MAAA,GAAA,CAAA,GAAA,IAAA,GAAA,KAAA;AACA,KAvHA;AAwHA,IAAA,eAAA,EAAA,yBAAA,KAAA,EAAA;AACA,aAAA,KAAA,IAAA,CAAA,GAAA,IAAA,GAAA,KAAA;AACA,KA1HA;AA2HA,IAAA,WAAA,EAAA,qBAAA,GAAA,EAAA;AACA,aAAA,GAAA,CAAA,MAAA,IAAA,CAAA,GAAA,IAAA,GAAA,KAAA;AACA;AA7HA;AAxBA,CAAA","sourcesContent":["<template>\n  <div class=\"singup\">\n    <header\n      class=\"page-header page-header-dark bg-img-repeat bg-info\"\n      style='background-image: url(\"assets/img/pattern-shapes.png\")'\n    >\n      <div class=\"page-header-content\">\n        <div class=\"container\">\n          <div class=\"row align-items-center\">\n            <div class=\"col-lg-5\">\n              <div\n                class=\"badge badge-marketing badge-pill badge-secondary-soft text-secondary mb-3\"\n              >\n                Empieza Ahora!\n              </div>\n              <h1 class=\"page-header-title\">Regístrate!</h1>\n              <p class=\"text-white\">\n                Solo para servidores de la Iglesia Calvary la Semilla.\n              </p>\n              <p class=\"text-white mt-5\">¿Ya tienes una cuenta?</p>\n              <router-link\n                class=\"btn btn-white btn-marketing rounded-pill\"\n                :to=\"{ path: '/login' }\"\n                >Iniciar Sesion!</router-link\n              >\n            </div>\n            <div class=\"col-lg-7\">\n              <div class=\"card rounded-lg text-dark\">\n                <div class=\"card-header py-4 text-dark\">\n                 Únete al equipo de Calvary la Semilla.\n                </div>\n                <div class=\"card-body\">\n                  <form @submit.prevent=\"register\">\n                    <div class=\"form-row\">\n                      <div class=\"form-group col-md-6\">\n                        <label\n                          class=\"small text-gray-600\"\n                          for=\"leadCapFirstName\"\n                          >Nombres</label\n                        >\n                        <input\n                          v-model=\"nombres\"\n                          class=\"form-control\"\n                          type=\"text\"\n                        />\n                      </div>\n                      <div class=\"form-group col-md-6\">\n                        <label class=\"small text-gray-600\" for=\"leadCapLastName\"\n                          >Apellidos</label\n                        >\n                        <input\n                          v-model=\"apellidos\"\n                          class=\"form-control\"\n                          type=\"text\"\n                        />\n                      </div>\n                    </div>\n                    <div class=\"form-row\">\n                      <div class=\"form-group col-md-6\">\n                        <label class=\"small text-gray-600\" for=\"leadCapLastName\"\n                          >Email</label\n                        >\n                        <input\n                          v-model=\"email\"\n                          class=\"form-control\"\n                          type=\"email\"\n                        />\n                      </div>\n                      <div class=\"form-group col-md-6\">\n                        <label class=\"small text-gray-600\" for=\"leadCapLastName\"\n                          >Contraseña</label\n                        >\n                        <input\n                          v-model=\"password\"\n                          class=\"form-control\"\n                          type=\"password\"\n                        />\n                      </div>\n                    </div>\n                    <button\n                      class=\"btn btn-primary btn-marketing btn-block rounded-pill mt-4\"\n                      type=\"submit\"\n                    >\n                      Registrarse!\n                    </button>\n                  </form>\n                </div>\n                <div class=\"card-footer\" v-if=\"error\">\n                  <span class=\"error-message text-danger\">\n                    {{ error_message }}\n                  </span>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"svg-border-angled text-white\">\n        <svg\n          xmlns=\"http://www.w3.org/2000/svg\"\n          viewBox=\"0 0 100 100\"\n          preserveAspectRatio=\"none\"\n          fill=\"currentColor\"\n        >\n          <polygon points=\"0,100 100,0 100,100\"></polygon>\n        </svg>\n      </div>\n    </header>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      dni: \"\",\n      nombres: \"\",\n      apellidos: \"\",\n      fotourl: \"https://www.flaticon.com/svg/static/icons/svg/813/813253.svg\",\n      tipousuario: \"0\",\n      email: \"\",\n      password: \"\",\n      estado: 0,\n      organizacion: 0,\n      error: false,\n      error_message: \"\",\n      organizaciones: {\n        data: [],\n        loanding: true\n      },\n      host: this.$store.getters.getHost\n    };\n  },\n  created() {\n    this.getOrganizaciones();\n  },\n  methods: {\n    getOrganizaciones: async function() {\n      this.organizaciones.loanding = true;\n      const response = await fetch(\n        this.host + \"/mayvoto/api/actions/organizaciones.php\"\n      );\n      const data = await response.json();\n      this.organizaciones.data = await data;\n      this.organizaciones.loanding = false;\n    },\n    verifyInputs() {\n      if (!this.validateText(this.nombres)) {\n        this.error = true;\n        this.error_message = \"Ingresa tu Nombre\";\n        return false;\n      }\n      if (!this.validateText(this.apellidos)) {\n        this.error = true;\n        this.error_message = \"Ingresa tus Apellidos\";\n        return false;\n      }\n      if (!this.validateDNI(this.dni)) {\n        this.error = true;\n        this.error_message = \"Pon un DNI Válido (8 dígitos)\";\n        return false;\n      }\n      if (!this.validateSelects(this.tipousuario)) {\n        this.error = true;\n        this.error_message = \"Escoje una opción\";\n        return false;\n      }\n      if (!this.validateSelects(this.organizacion)) {\n        this.error = true;\n        this.error_message = \"Escoja una opción\";\n        return false;\n      }\n      if (!this.validateEmail(this.email)) {\n        this.error = true;\n        this.error_message = \"Ingrese un corre electrónico válido\";\n        return false;\n      }\n      if (!this.validatePass(this.password)) {\n        this.error = true;\n        this.error_message = \"La contraseña debe ser mayor a 4 dígitos\";\n        return false;\n      }\n      return true;\n    },\n    async register() {\n      if (this.verifyInputs()) {\n        //call axios to sing up\n        // then go to singin ==== login\n        let _data = new FormData();\n        _data.set(\"dni\", this.dni);\n        _data.set(\"nombres\", this.nombres);\n        _data.set(\"apellidos\", this.apellidos);\n        _data.set(\"fotourl\", this.fotourl);\n        _data.set(\"tipousuario\", this.tipousuario);\n        _data.set(\"email\", this.email);\n        _data.set(\"password\", this.password);\n        _data.set(\"estado\", this.estado);\n        _data.set(\"organizacion\", this.organizacion);\n        // console.log(_user.username +' | '+ _user.password)\n        axios({\n          method: \"post\",\n          url: this.host + \"/mayvoto/api/actions/register.php\",\n          data: _data,\n          headers: {\n            \"content-type\": \"application/x-www-form-urlencoded\"\n          }\n        })\n          .then(response => {\n            //handle response\n            if (!response.data.error) {\n              if (this.tipousuario == \"Votante\") {\n                this.$router.replace(this.$route.query.redirect || \"/login\");\n              }\n              if (this.tipousuario == \"Organizador\") {\n                this.$router.replace(\n                  this.$route.query.redirect ||\n                    \"/login?redirect=%2Forganizadores\"\n                );\n              }\n              if (this.tipousuario == \"Supervisor\") {\n                this.$router.replace(\n                  this.$route.query.redirect ||\n                    \"/login?redirect=%2Fsupervisores\"\n                );\n              }\n            } else {\n              //api sends 2 type of errors by code\n              //1: Email exists; 2: Error while inserting\n              this.error = true;\n              if (response.data.code == 1) {\n                this.error_message =\n                  \"El correo eléctronico o DNI ya esta registrado, intenta con otro!\";\n              } else {\n                this.error_message =\n                  \"Upps, tenemos problemas para registrarte!\";\n              }\n            }\n          })\n          .catch(response => {\n            //handle error\n            this.error = true;\n            console.log(response);\n          });\n      }\n    },\n    validateEmail: function(email) {\n      var re = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\n\n      return re.test(email);\n    },\n    validateText: function(text) {\n      return text.length > 1 ? true : false;\n    },\n    validatePass: function(pass) {\n      return pass.length > 3 ? true : false;\n    },\n    validateSelects: function(value) {\n      return value != 0 ? true : false;\n    },\n    validateDNI: function(str) {\n      return str.length == 8 ? true : false;\n    }\n  }\n};\n</script>\n<style scoped>\n.icon-gender {\n  width: 50px;\n}\n</style>\n"],"sourceRoot":"src/views"}]}