reEnter: requireAuth\n  }, \n  {\n    path: \"/votar\",\n    name: \"votar\",\n    props: true,\n    component: () => import(/* webpackChunkName: \"cursos\" */ \"../views/votar/Index.vue\"),\n    beforeEnter: requireAuth\n  },\n  {\n    path: \"/votar/:id\",\n    name: \"elegir\",\n    props: true,\n    component: () => import(/* webpackChunkName: \"cursos\" */ \"../views/votar/Eligir.vue\"),\n    beforeEnter: requireAuth\n  },\n  {\n    path: \"/perfil\",\n    name: \"perfil\",\n    component: () =>\n      import(/* webPackChunkName: \"Dashboard\" */ \"../views/Perfil\"),\n    beforeEnter: requireAuth\n  },\n  {\n    path: \"/login\",\n    name: \"login\",\n    component: () =>\n      import(/* webPackChunkName: \"Login\" */ \"../views/Login.vue\")\n  },\n  {\n    path: \"/registrarse\",\n    name: \"registrarse\",\n    component: () =>\n      import(/* webPackChunkName: \"Login\" */ \"../views/Registrarse.vue\")\n  },\n  {\n    path: \"/logout\",\n    beforeEnter(to, from, next) {\n      auth.logout();\n      next(\"/\");\n    }\n  },\n  {\n    path: \"*\",\n    name: \"notfound\",\n    props: true,\n    component: () =>\n      import(/* webpackChunkName: \"clases\" */ \"../views/NotFound\")\n  }\n];\n\nconst router = new VueRouter({\n  mode: \"history\",\n  linkExactActiveClass: \"studia-active-link\",\n  routes\n});\n\nexport default router;\n"]}]}